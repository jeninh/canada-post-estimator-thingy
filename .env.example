# Canada Post API Credentials
CP_API_USERNAME=your_api_username
CP_API_PASSWORD=your_api_password
CP_CUSTOMER_NUMBER=your_customer_number
CP_CONTRACT_ID=your_contract_id

# Origin postal code (your shipping location)
ORIGIN_POSTAL_CODE=A1A1A1

# Environment: 'sandbox' or 'production'
# Note: Use 'production' with your production API keys
CP_ENVIRONMENT=sandbox

# Server port
PORT=3000
